<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="ImageButtonStyle1">
        <item name="android:layout_width">@dimen/icon_extremely_extra_large</item>
        <item name="android:layout_height">@dimen/icon_extremely_extra_large</item>
        <item name="android:background">@drawable/btn_home</item>
        <item name="android:scaleType">centerInside</item>
        <item name="android:src">@drawable/ic_message</item>
    </style>

    <style name="TextViewStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="TextViewCommon">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="EditTextCommon">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
    </style>


    <style name="TextViewStyle01" parent="TextViewStyle">
        <item name="android:textColor">@color/black</item>
    </style>


    <style name="TextViewStyle02" parent="TextViewStyle01">
        <item name="android:textSize">@dimen/text_small_plus</item>
    </style>

</resources>